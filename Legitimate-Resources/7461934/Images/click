window.getCookie = function(name) {
    match = document.cookie.match(new RegExp(name + '=([^;]+)'));
    if (match) return match[1];
}
$(document).ready(function() {
    
        // Billing JSON Logic begins
    
        const json_url = "https://quickbooks.intuit.com/qbmds-data/us/billing_offers_us.json?v4=5";
        
        var request = new XMLHttpRequest();
        request.open('GET', json_url, true);
        
        request.onload = function() {
        
            // json path and html id
            var codes_source = [
                ["data.campaigns.default.default.QBO.QBO_SIMPLE_START.MONTHLY.PAID.offer_id", "6564"],
                ["data.campaigns.default.default.QBO.QBO_PLUS.MONTHLY.PAID.offer_id", "8113"],
                ["data.campaigns.default.default.QBO.QBO_ADVANCED.MONTHLY.PAID.offer_id", "9252"],
                ["data.campaigns.default.default.PAYROLL.PR_CORE.MONTHLY.PAID.offer_id", "6930"],
                ["data.campaigns.default.default.PAYROLL.PR_PREMIUM.MONTHLY.PAID.offer_id", "9060"],
                ["data.campaigns.default.default.PAYROLL.PR_ELITE.MONTHLY.PAID.offer_id", "6485"],
            ];
            if( window.getCookie("mds_3rdparty_experience_store")){
                if( window.getCookie("mds_3rdparty_experience_store").indexOf("43119") > -1){
                  codes_source = [
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.QBO.QBO_SIMPLE_START.MONTHLY.PAID.offer_id", "6564"],
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.QBO.QBO_PLUS.MONTHLY.PAID.offer_id", "8113"],
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.QBO.QBO_ADVANCED.MONTHLY.PAID.offer_id", "9252"],
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.PAYROLL.PR_CORE.MONTHLY.PAID.offer_id", "6930"],
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.PAYROLL.PR_PREMIUM.MONTHLY.PAID.offer_id", "9060"],
                    ["data.campaigns.QB_QBKS_30047_WB_RW_QBO_Price_Increase_Holdout.Treatment2.PAYROLL.PR_ELITE.MONTHLY.PAID.offer_id", "6485"],
                ];
                }
            }
            document.getElementById("codes_source").innerHTML = JSON.stringify(codes_source);
   
            const path_to_offers = "data.offerDefinitions";
            document.getElementById("path_to_offers").innerHTML = path_to_offers;
            
            var codes = [];
        
            if (request.status >= 200 && request.status < 400) {
    
                var data = JSON.parse(request.responseText);
        
                document.getElementById("billing_offers_us_json").innerHTML = request.responseText;
    
                for (let index = 0; index < codes_source.length; index++) {
                    const offer_id = eval(codes_source[index][0]);
                    codes.push([
                        eval(path_to_offers + "[" + offer_id.toString() + "]"), // actual data from json
                        codes_source[index][1] // html ID
                    ]);
                }
        
                for (let index = 0; index < codes.length; index++) {
        
                    const data_from_json = codes[index][0];
                    const html_id = codes[index][1];
        
                    if (typeof(data_from_json.basePrice)!="undefined" &&
                        typeof(data_from_json.discountPriceWhole)!="undefined" &&
                        typeof(data_from_json.discountPriceCents)!="undefined") {
        
                        if (document.getElementById("reg-price-dollar-"+html_id) != null) 
                            document.getElementById("reg-price-dollar-"+html_id).innerHTML = data_from_json.basePrice;
                        if (document.getElementById("dis-price-dollar-"+html_id) != null) 
                            document.getElementById("dis-price-dollar-"+html_id).innerHTML = data_from_json.discountPriceWhole;
                        if (document.getElementById("dis-price-cents-"+html_id) != null) {
                            if (data_from_json.discountPriceCents=="00") {
                                document.getElementById("dis-price-cents-"+html_id).innerHTML = "";
                            } else {
                                document.getElementById("dis-price-cents-"+html_id).innerHTML = data_from_json.discountPriceCents;
                            }
                        }
        
                        // does it have perWorkerCharge 
                        if (data_from_json.perWorkerCharge &&
                            document.getElementById("offer-text-"+html_id) != null &&
                            typeof(data_from_json.perWorkerCharge)!="undefined") {
                                document.getElementById("offer-text-"+html_id).innerHTML = "+$"+data_from_json.perWorkerCharge+"/employee per month";
                        }
        
                    }
                }
            }
        };
        
        request.onerror = function() {
        // There was a connection error of some sort
        };
        request.send();
});
</script>
