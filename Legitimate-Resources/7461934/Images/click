                       if (typeof l === "string" || typeof l === "number") {
                           l = e("<div></div>").html(l);
                       } else {
                           alert("SimpleModal Error: Unsupported data type: " + typeof l);
                           return k;
                       }
                   }
                   k.create(l);
                   l = null;
                   k.open();
                   if (e.isFunction(k.o.onShow)) {
                       k.o.onShow.apply(k, [k.d]);
                   }
                   return k;
               },
               create: function (k) {
                   var d = this;
                   d.getDimensions();
                   if (d.o.modal && b.ie6) {
                       d.d.iframe = e('<iframe src="javascript:false;"></iframe>')
