<span id="the_json_source" style="display: none;"></span>
<script>
  var the_json;
jQuery(document).ready(function(e) {
    try {
        e = new XMLHttpRequest;
        const r = "https://quickbooks.intuit.com/qbmds-components/pricing/us/oa-master-features-phoenix-new.json";
        e.open("GET", r, !1), e.onreadystatechange = function() {
            4 === e.readyState && (the_json = JSON.parse(e.responseText))
        }, e.send()
    } catch (e) {
        console.log("error", e)
    }
    jQuery("#open-popup-video").click(function(e) {
        const r = jQuery("#open-popup-video").attr("data-video-id");
        r && (jQuery("#drawer-video-iframe").attr("src", "https://www.youtube.com/embed/" + r + "?rel=0&modestbranding=1&autoplay=1"), jQuery("#drawer-video-modal").show())
    }), jQuery(document).on( "click", ".open-popup", function(e) {
        if (the_json) {
            if (jQuery(this).hasClass( "quiz-open-popup" )) {
                var dataValue = jQuery(this).attr("data-popup");
            }else {
                var dataValue = jQuery(this).attr("data-popup");
            }
            if (dataValue) {
                const popup_data = the_json.featureDrawers[dataValue];
                jQuery("#drawer-popup-name").html(popup_data.drawerName);
                const headline = '<' +
                    popup_data.drawerHeadline.as +
                    ' data-testid="heading" class="Heading_module_heading3__1032c897 Heading_module_demi__1032c897 left Heading_module_greyColor__1032c897">' +
                    popup_data.drawerHeadline.children +
                    '</'+popup_data.drawerHeadline.as+'>';
                jQuery("#drawer-popup-headline").html(headline);
                jQuery("#drawer-popup-list").html(popup_data.drawerRespText.text);
                if (popup_data.drawerFootnote.text && jQuery("#drawerfootNoteText")) {
                    jQuery("#drawerfootNoteText").html(popup_data.drawerFootnote.text);
                }
                if (popup_data.showVideo) {
                    jQuery("#drawer-video-thumb").attr("src", popup_data.drawerVideo.mediaDesktopThumbnail);
                    jQuery("#open-popup-video").attr("data-video-id", popup_data.drawerVideo.id);
                    jQuery("#drawer-img").hide();
                    jQuery("#drawer-video").show();
                } else {
                    jQuery("#drawer-mobile-img").attr("srcset", popup_data.drawerImage.mediaMobileUrl);
                    jQuery("#drawer-tablet-img").attr("srcset", popup_data.drawerImage.mediaTabletUrl);
                    jQuery("#drawer-desktop-img").attr("src", popup_data.drawerImage.mediaDesktopUrl);
                    jQuery("#drawer-video").hide();
                    jQuery("#drawer-img").show();
                }
                document.getElementById('popup-template').style.display = "flex";
            }
        }
    })
}), window.onclick = function(e) {
    console.log(e.target.id), "drawer-overlay" == e.target.id && jQuery("#popup-template").hide(), "drawer-video-overlay" == e.target.id && (jQuery("#drawer-video-iframe").attr("src", ""), jQuery("#drawer-video-modal").hide()), -1 != e.target.id.indexOf("drawer-close-btn") && jQuery("#popup-template").hide(), -1 != e.target.id.indexOf("drawer-video-modal") && (jQuery("#drawer-video-iframe").attr("src", ""), jQuery("#drawer-video-modal").hide())
};
$(document).ready(function() {
    var oalivephnum = document.querySelectorAll("a[data-wa-link='ftr-quickbooks-sitemap']");
	if (oalivephnum) {
		for (var i = 0; i < oalivephnum.length; i++) {
		    oalivephnum[i].classList.add('rTapNumber406079');
		}
	}
});
</script>

