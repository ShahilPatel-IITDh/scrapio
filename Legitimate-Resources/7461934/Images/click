                       .attr("id", d.o.overlayId)
                       .addClass("simplemodal-overlay")
                       .css(e.extend(d.o.overlayCss, { display: "none", height: d.o.modal ? j[0] : 0, width: d.o.modal ? j[1] : 0, position: "fixed", left: 0, top: 0, zIndex: d.o.zIndex + 1 }))
