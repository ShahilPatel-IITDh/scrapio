                       l = l instanceof e ? l : e(l);
                       k.d.placeholder = false;
                       if (l.parent().parent().length > 0) {
                           l.before(e("<span></span>").attr("id", "simplemodal-placeholder").css({ display: "none" }));
                           k.d.placeholder = true;
                           k.display = l.css("display");
                           if (!k.o.persist) {
                               k.d.orig = l.clone(true);
                           }
                       }
