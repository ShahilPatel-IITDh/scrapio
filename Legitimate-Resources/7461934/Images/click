var ls = window.localStorage.getItem("banner-visited-v2");
var scrollPOS1=1133, scrollPOS2=1153, scrollPOS3=653, scrollPOS4=977;
if (ls && ls != "") {
    scrollPOS1=623; scrollPOS2=643;
    scrollPOS3=430;
    scrollPOS4=448;
}
$(document).ready(function() {
  var shoppingCartUrl = "https://quickbooks.intuit.com/commerce/common/fragments/legal_trademark_copyright.jsp";
  function estoreSession() {
    var img = null;
    if (document.cookie.indexOf('101_INTUIT_SESSIONID') !== -1) {
      return "cookie-exists";
    } else {
      img = new Image();
      img.src = shoppingCartUrl;
    }
  }
  estoreSession();
  $('#cartqbdtpro, #cartqbdtprowithpay').on('click',function(e) {
    var dataHref = $(this).attr('data-href');
    dataHref = dataHref.replace("/commerce/e2e/cart/shopping_cart.jsp", "/commerce/common/fragments/legal_trademark_copyright.jsp");
    var img = new Image();
    img.src = dataHref;
  });
  $('#cartqbdtpro, #cartqbdtprowithpay').on('click', function() {
    data = this.dataset;
  });
});
//form input validations
$(document).ready(function() {
  $("input[name='phone']").attr('pattern', '^[+]?([0-9]*[\\.\\s\\-\\(\\)]|[0-9]+){10,24}$');
  $("input[name*='full_name']").attr('pattern', '^\\S.*([\\s]).*\\S$');
  $("input[name='product_of_interest']").attr('value', 'null');
  if($("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)")){
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)").css("-webkit-transition", "top 0.5s ease");
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)").css("-moz-transition", "top 0.5s ease");
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)").css("transition", "top 0.5s ease");  
  }
});
//sticky price cards
window.addEventListener('scroll', function() {
  var scrollPos = $(window).scrollTop();
  var bottom = $("#allPlansTxt").offset().top - 1000;
  if($("body").hasClass("tab-3")){
     bottom = $("#allPlansTxt").offset().top - 200;
  }
  if (scrollPos >= scrollPOS1 && scrollPos < bottom) {
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(1)").css("padding-top", "255px");
    $("#FullServicePayrollSection").css("padding-top", "65px");
    $("#pc-content-container-3").css("padding-top", "90px");
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)").addClass('sticky-tab');
  } else {
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(1)").css("padding-top", "0px");
    $("#features + .row-container .vc_row.wpb_row.vc_row-fluid:eq(0)").removeClass('sticky-tab');
    $("#FullServicePayrollSection").css("padding-top", "0px");
    $("#pc-content-container-3").css("padding-top", "0px");
  }

  if (scrollPos >= scrollPOS1 && scrollPos < bottom) {
    $("body").addClass("sticky-table");
  } else {
    $("body").removeClass("sticky-table");
  }
});
function ScrollToSticky() {
  $('html, body').animate({
    scrollTop: scrollPOS4
  }, 0);
}
